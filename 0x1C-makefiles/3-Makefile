#this programa all
CC: gcc
SRC: main.c holberton.c
OBJ: $(SRC:.c=.o)
NAME: holberton
RM: rm

all: programa

programa: $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

%.o : %.c:
	$(CC) -c $< -o $@ 

clean:
	$(RM) -f *~ $(NAME)

oclean:
	$(RM) -f $(OBJ)

fclean:
	$(RM) -f  *.~ $(NAME) 
	$(RM) -f $(OBJ)

re: oclean all